{
    "name": "Meow You See Me",
    "version": "1.2.0",
    "icons": {
        "16": "cat_16.png",
        "32": "cat_32.png",
        "48": "cat_48.png",
       "128": "cat_128.png"
    },
    "description": "He do be hiding tho",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js", "jsoncatdata.js", "specialFunctions.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Meow You See Me"
    }
}