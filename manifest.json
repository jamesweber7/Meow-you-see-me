{
    "name": "Meow You See Me",
    "version": "0.0.1",
    "icons": {
        "16": "cat_16.png",
        "32": "cat_32.png",
        "48": "cat_48.png",
       "128": "cat_128.png"
    },
    "description": "He do be hiding tho",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Meow You See Me"
    }
}